#include <stdio.h>
#include <stdbool.h>

// Function to check if a number is prime
bool isPrime(int num) {
    if (num <= 1) {
        return false;
    }
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return false;
        }
    }
    return true;
}

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    int count = 0;
    char str[20];  // To store the string representation of the number
    snprintf(str, sizeof(str), "%d", n);  // Convert the number to a string

    // Iterate through the string and form two-digit numbers
    for (int i = 0; str[i + 1] != '\0'; i++) {
        int twoDigitNumber = (str[i] - '0') * 10 + (str[i + 1] - '0');

        // Check if the two-digit number is prime
        if (isPrime(twoDigitNumber)) {
            count++;
        }
    }

    // Output the result
    printf("Number of two-digit prime numbers: %d\n", count);

    return 0;
}
